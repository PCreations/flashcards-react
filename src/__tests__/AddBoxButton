import React from 'react';
import { act } from 'react-testing-library';
import userEvent from 'user-event';
import { createRender } from './createRender';
import { AddBoxButton } from '../AddBoxButton';

describe('AddBoxButton', () => {
  test(`
    given the current route is HOME
    when clicking the AddBoxButton
    then the current route should be NEW_BOX
  `, () => {
    const render = createRender({});
    const { getByLabelText } = render(<AddBoxButton/>);
    act(() => {
      userEvent.click(getByLabelText('create a new box'));
    });
    expect()
  });
});
